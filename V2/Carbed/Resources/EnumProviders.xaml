<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib"
                    xmlns:StatDataLogic="clr-namespace:Carbon.Engine.Resource.Content;assembly=Carbon.Engine"
                    xmlns:processors="clr-namespace:Carbon.Editor.Processors;assembly=Carbon.Editor"
                    xmlns:drawing="clr-namespace:System.Drawing;assembly=System.Drawing">
    
    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type System:Enum}" x:Key="ResourceTypes">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="StatDataLogic:ResourceType" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type System:Enum}" x:Key="TextureTargetFormat">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="processors:TextureTargetFormat" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type System:Enum}" x:Key="FontStyle">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="drawing:FontStyle" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
</ResourceDictionary>