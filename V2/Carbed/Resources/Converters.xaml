<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:docking="clr-namespace:Carbed.Logic.Docking" 
                    xmlns:mvvm="clr-namespace:Carbed.Logic.MVVM">

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    
    <docking:ActiveDocumentConverter x:Key="ActiveDocumentConverter"/>
    
    <mvvm:NullToBooleanConverter x:Key="NullToBooleanConverter"/>

    <mvvm:ReferenceToTypeConverter x:Key="ReferenceToTypeConverter"/>
    
    <mvvm:UriToBitmapFrameConverter x:Key="UriToBitmapFrameConverter"/>
    <mvvm:UriToImageConverter x:Key="UriToImageConverter"/>

    <mvvm:BooleanToColorConverter x:Key="BooleanToColorConverter"/>

</ResourceDictionary>